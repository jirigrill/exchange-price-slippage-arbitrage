          "format": "time_series",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH price_comparison AS (\n  SELECT \n    k.timestamp,\n    k.price_usd as kraken_price,\n    c.price_usd as coinmate_price,\n    ((c.price_usd - k.price_usd) / k.price_usd) * 100 as spread_pct\n  FROM exchange_prices k\n  JOIN exchange_prices c ON \n    ABS(EXTRACT(EPOCH FROM (c.timestamp - k.timestamp))) <= 60\n  WHERE k.exchange_name = 'kraken' \n    AND c.exchange_name = 'coinmate'\n    AND $__timeFilter(k.timestamp)\n)\nSELECT \n  timestamp AS \"time\",\n  CASE \n    WHEN spread_pct < -3.0 THEN 'STRONG_BUY_coinmate'\n    WHEN spread_pct < -1.5 THEN 'BUY_on_coinmate'\n    WHEN spread_pct > 3.0 THEN 'STRONG_SELL_coinmate'\n    WHEN spread_pct > 1.5 THEN 'SELL_on_coinmate'\n    ELSE NULL\n  END as metric,\n  ABS(spread_pct) as value\nFROM price_comparison\nWHERE ABS(spread_pct) > 1.5\nORDER BY timestamp",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "profit_percentage"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "exchange_prices",
          "timeColumn": "timestamp",
          "timeColumnType": "timestamptz",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
      ],
